{
  "name": "URLer",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Put URLs to work for you",
  "homepage_url": "http://urler.herokuapp.com.com",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "background": {
    "page": "src/bg/background.html",
    "persistent": false
  },
  "options_page": "src/options_custom/index.html",
  "browser_action": {
    "default_icon": "icons/icon19.png",
    "default_title": "WTF",
    "default_popup": "src/browser_action/browser_action.html"
  },
  "permissions": [
    "contextMenus"
  ]
}